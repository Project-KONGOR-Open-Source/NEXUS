{
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "profiles": {
        "ASPIRE.AppHost Development": {
            "commandName": "Project",
            "launchBrowser": true,
            "applicationUrl": "https://localhost:55500",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development",
                "DOTNET_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:55501",
                "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:55502"
            },
            "dotnetRunMessages": true
        },
        "ASPIRE.AppHost Production": {
            "commandName": "Project",
            "launchBrowser": true,
            "applicationUrl": "https://aspire.kongor.online",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Production",
                "DOTNET_ENVIRONMENT": "Production",
                "DOTNET_DASHBOARD_OTLP_ENDPOINT_URL": "https://telemetry.kongor.online",
                "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://resources.kongor.online"
            },
            "dotnetRunMessages": true
        }
    }
}
