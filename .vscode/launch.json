{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Aspire Application Host",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ASPIRE.ApplicationHost/bin/Debug/net10.0/ASPIRE.ApplicationHost.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ASPIRE.ApplicationHost",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
            "enableStepFiltering": false,
            "suppressJITOptimizations": true,
            "requireExactSource": false
        }
    ]
}
